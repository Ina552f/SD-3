package com.company;

public class CourseData {
    String StudentID;
    Integer Grade;
    String StudentName;
    String ID;
    String Name;
    String Semester;
    Integer Year;
    String Teacher;

    public CourseData(String StudentID, Integer Grade, String StudentName, String CourseID, String Name, String Semester, Integer Year, String Teacher) {
        this.StudentID = StudentID;
        this.Grade = Grade;
        this.StudentName = StudentName;
        this.CourseID = CourseID;
        this.Name = Name;
        this.Semester = Semester;
        this.Year = Year;
        this.Teacher = Teacher;
    }

    public String getCID() {
        return CourseID;
    }

    // String that display selected information to the user interface
    @Override
    public String toString() {
        return SID + " " + Grade + " " + StudentName + " " + CID + " " + CourseName
                + " " + Semester + " " + Year + " " + Teacher;
    }
}
